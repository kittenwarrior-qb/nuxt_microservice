{
  "name": "thegioididong-backend",
  "version": "1.0.0",
  "description": "Microservice backend for TheGioiDiDong using Moleculer and PostgreSQL",
  "main": "index.js",
  "scripts": {
    "dev": "node index.js",
    "dev:moleculer": "moleculer-runner --repl --hot services/**/*.service.js", 
    "start": "node index.js",
    "cli": "moleculer connect NATS",
    "test": "jest",
    "test:watch": "jest --watch",
    "migrate": "moleculer-runner --repl services/migration.service.js",
    "lint": "eslint services",
    "dc:up": "docker-compose up --build -d",
    "dc:down": "docker-compose down"
  },
  "keywords": [
    "microservices",
    "moleculer",
    "postgresql",
    "nodejs",
    "api"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express-rate-limit": "^6.10.0",
    "firebase-admin": "^12.0.0",
    "helmet": "^7.0.0",
    "ioredis": "^5.7.0",
    "joi": "^17.9.2",
    "jsonwebtoken": "^9.0.2",
    "moleculer": "^0.14.29",
    "moleculer-db": "^0.8.23",
    "moleculer-db-adapter-sequelize": "^0.2.16",
    "moleculer-web": "^0.10.5",
    "nats": "^2.29.3",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.32.1"
  },
  "devDependencies": {
    "@types/node": "^24.4.0",
    "eslint": "^8.46.0",
    "jest": "^29.6.2",
    "moleculer-repl": "^0.7.4",
    "nodemon": "^3.0.1",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "jest": {
    "testEnvironment": "node",
    "rootDir": "./",
    "testMatch": [
      "**/test/**/*.spec.js"
    ]
  }
}
